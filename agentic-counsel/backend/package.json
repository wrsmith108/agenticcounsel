{
  "name": "agentic-counsel-backend",
  "version": "1.0.0",
  "description": "AI-powered executive coaching platform backend",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "db:migrate": "ts-node src/database/setup.ts migrate",
    "db:status": "ts-node src/database/setup.ts status",
    "db:rollback": "ts-node src/database/setup.ts rollback",
    "db:seed": "ts-node src/database/seed.ts seed",
    "db:clear": "ts-node src/database/seed.ts clear",
    "db:setup": "npm run db:migrate && npm run db:seed"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@types/bcrypt": "^5.0.2",
    "astronomia": "^4.1.1",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "ephemeris": "^2.0.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "openai": "^4.20.1",
    "pg": "^8.11.3",
    "prisma": "^5.7.1",
    "redis": "^4.6.11",
    "socket.io": "^4.7.4",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.4",
    "@types/pg": "^8.15.4",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "ai",
    "coaching",
    "personality",
    "executive-coaching",
    "nodejs",
    "typescript"
  ],
  "author": "Agentic Counsel Team",
  "license": "MIT"
}
