{
  "name": "@agentic-counsel/shared",
  "version": "1.0.0",
  "description": "Shared types, utilities, and configurations for Agentic Counsel",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "typescript",
    "types",
    "shared",
    "monorepo"
  ],
  "author": "Agentic Counsel Team",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.0.0"
  }
}